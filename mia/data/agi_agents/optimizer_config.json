    "cpu": {
      "enabled": true,
      "priority": "medium",
      "auto_trigger": true
    },
    "disk": {
      "enabled": true,
      "priority": "medium",
      "auto_trigger": false
    },
    "network": {
      "enabled": true,
      "priority": "low",
      "auto_trigger": false
    },
    "algorithm": {
      "enabled": true,
      "priority": "medium",
      "auto_trigger": false
    },
    "resource": {
      "enabled": true,
      "priority": "high",
      "auto_trigger": true
    },
    "workflow": {
      "enabled": true,
      "priority": "medium",
      "auto_trigger": false
    }
